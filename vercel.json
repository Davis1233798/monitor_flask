{
  "version": 2,
  "builds": [
    {
      "src": "api/monitor.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/monitor.py"
    }
  ],
  "env": {
    "TELEGRAM_TOKEN": "${TELEGRAM_TOKEN}",
    "TELEGRAM_CHAT_ID": "${TELEGRAM_CHAT_ID}",
    "DISCORD_WEBHOOK_URL": "${DISCORD_WEBHOOK_URL}",
    "WEBCRAWLER_DISCORD_WEBHOOK_URL": "${WEBCRAWLER_DISCORD_WEBHOOK_URL}"
  }
}